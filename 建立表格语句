create table student(sid int primary key auto_increment,sname varchar(20), college varchar(20), tel varchar(20)ï¼Œsex varchar(10))charset='utf8';
create table study(sid int,sname varchar(20),cname varchar(20),degree int,lagtime int,lapsetimes int)charset=utf8;
create table class(cname varchar(20), rname varchar(20), lesson varchar(20))charset='utf8';
create table job(sid int,sanme varchar(20),graduatetime date,salary varchar(20),company varchar(20))charset=utf8;
create table hopping(
sname varchar(20),
newcompany varchar(60),
newsalary int,
hoppingtime date,
sid int	
)charset=utf8;